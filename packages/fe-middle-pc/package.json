{
    "name": "fe-middle-pc",
    "private": false,
    "description": "平台pc中台",
    "scripts": {
        "prefix": "lessc --js --modify-var=@border-radius-base=6px --modify-var=@checkbox-border-radius=2px node_modules/antd/dist/antd.variable.less src/styles/antd.variable.css && node prefixCss.js",
        "start": "npm run prefix && cross-env BUILD_ENV=local ALIAS_ENV=ceshi1 umi dev",
        "start:egz": "npm run prefix && cross-env BUILD_ENV=local ALIAS_ENV=egz umi dev",
        "start:enx": "npm run prefix && cross-env BUILD_ENV=local ALIAS_ENV=enx umi dev",
        "start:w": "npm run prefix && cross-env BUILD_ENV=local ALIAS_ENV=w umi dev",
        "start:test": "npm run prefix && cross-env BUILD_ENV=local ALIAS_ENV=test umi dev",
        "dev": "node initDomain && npm run prefix && cross-env BUILD_ENV=dev ALIAS_ENV=global umi build",
        "test": "node initDomain && npm run prefix && cross-env BUILD_ENV=test ALIAS_ENV=global umi build",
        "pre": "node initDomain && npm run prefix && cross-env BUILD_ENV=pre ALIAS_ENV=global umi build",
        "pro": "node initDomain && npm run prefix && cross-env BUILD_ENV=pro ALIAS_ENV=global umi build",
        "test:coverage": "umi-test --coverage",
        "analyze": "cross-env ANALYZE=1 BUILD_ENV=pro ALIAS_ENV=global umi build",
        "lint": "echo \"Please run lint from root directory: pnpm lint\"",
        "lint:all": "echo \"Please run lint:all from root directory: pnpm lint:all\"",
        "middle": "node initDomain && npm run prefix && umi dev",
        "enroll": "node initDomain && npm run prefix && umi dev",
        "job": "node initDomain && npm run prefix && umi dev",
        "all": "node initDomain && eslint src --fix && npm run prefix && umi dev",
        "prepare": "husky install",
        "gx": "umi dev"
    },
    "dependencies": {
        "js-base64": "^2.6.4",
        "@ant-design/icons": "5.0.1",
        "@ant-design/pro-layout": "6.5.0",
        "@dnd-kit/core": "^6.0.8",
        "@dnd-kit/sortable": "^7.0.2",
        "@dnd-kit/utilities": "^3.2.1",
        "@types/lodash": "^4.17.7",
        "@types/react-beautiful-dnd": "^13.1.3",
        "@umijs/plugin-qiankun": "2.42.0",
        "@wangeditor/editor": "^5.1.23",
        "@wangeditor/editor-for-react": "^1.0.6",
        "@wotu/pt-components": "1.5.1-beta.0.1.31",
        "@wotu/wotu-components": "1.8.4-beta.0.2.16",
        "@wotu/wotu-pro-components": "0.2.11-beta.0.0.4",
        "ahooks": "^3.7.0",
        "antd": "^4.24.0",
        "axios": "0.19.2",
        "classnames": "^2.3.2",
        "clipboard": "^2.0.11",
        "dayjs": "^1.11.2",
        "html2canvas": "1.4.1",
        "immutability-helper": "^3.1.1",
        "js-cookie": "2.2.0",
        "lodash": "^4.17.21",
        "mobx": "6.3.2",
        "mobx-persist": "^0.4.1",
        "mobx-persist-store": "^1.0.6",
        "mobx-react": "7.2.0",
        "moment": "^2.29.4",
        "path-to-regexp": "1.8.0",
        "qrcode.react": "^3.1.0",
        "rc-picker": "^3.5.1",
        "react": "17.0.2",
        "react-beautiful-dnd": "13.1.1",
        "react-color": "^2.19.3",
        "react-dnd": "^16.0.1",
        "react-dnd-html5-backend": "^16.0.1",
        "react-dom": "17.0.2",
        "react-svg": "^16.1.22",
        "reflect-metadata": "0.1.13",
        "stylelint-config-recommended": "^12.0.0",
        "umi": "3.5.20",
        "uuid": "^9.0.0",
        "stylelint-config-css-modules": "^4.5.1"
    },
    "devDependencies": {
        "@babel/compat-data": "7.21.4",
        "@types/react": "^17.0.75",
        "@types/react-dom": "^17.0.25",
        "@types/uuid": "^9.0.1",
        "@umijs/plugin-esbuild": "1.4.1",
        "@umijs/preset-react": "1.x",
        "@umijs/test": "3.2.20",
        "antd-theme-webpack-plugin": "1.3.6",
        "babel-plugin-transform-remove-console": "6.9.4",
        "chalk": "3.0.0",
        "check-prettier": "1.0.3",
        "clean-webpack-plugin": "4.0.0",
        "compression-webpack-plugin": "6.0.2",
        "cross-env": "7.0.3",
        "less": "^4.1.3",
        "stylelint-config-rational-order": "^0.1.2",
        "stylelint-config-recess-order": "^4.0.0",
        "typescript": "^4.9.5",
        "webpack": "5.6.0",
        "webpack-cli": "4.2.0",
        "@commitlint/cli": "^17.8.1",
        "@commitlint/config-conventional": "^17.8.1",
        "husky": "^8.0.3",
        "lint-staged": "^10.5.4",
        "prettier": "^2.8.8",
        "stylelint": "^15.11.0",
        "eslint": "^8.18.0"
    },
    "author": "feelingzrs",
    "resolutions": {
        "@types/react": "17.0.0",
        "@types/react-dom": "17.0.0"
    },
    "#overrides说明#": "淘宝源path-to-regexp1.8.0版本与1.9.0版本错乱处理，勿删",
    "overrides": {
        "path-to-regexp@1.8.0": {
            "resolveFrom": "npm",
            "source": "https://registry.npmjs.org/path-to-regexp/-/path-to-regexp-1.8.0.tgz"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": [
            "prettier --write",
            "eslint --fix"
        ],
        "src/**/*.{less,css}": [
            "prettier --write",
            "stylelint --fix"
        ],
        "src/**/*.{md,json}": [
            "prettier --write"
        ],
        "*.{json,js,ts}": [
            "prettier --write"
        ]
    }
}
