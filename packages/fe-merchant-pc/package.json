{
  "name": "fe-merchant-pc",
  "private": false,
  "version": "1.0.0",
  "scripts": {
    "job": "node initDomain && npm run prefix && umi dev",
    "prefix": "lessc --js node_modules/antd/dist/antd.variable.less src/styles/antd.variable.css && node prefixCss.js",
    "start": "node initDomain && eslint src --fix && npm run prefix && cross-env BUILD_ENV=local ALIAS_ENV=merchant umi dev",
    "merchant": "node initDomain && npm run prefix &&  BUILD_ENV=local && NODE_OPTIONS=--max-old-space-size=4096 && umi dev",
    "dev": "node initDomain && npm run prefix && cross-env BUILD_ENV=dev ALIAS_ENV=global NODE_OPTIONS=--max-old-space-size=4096 umi build",
    "test": "node initDomain && npm run prefix && cross-env BUILD_ENV=test ALIAS_ENV=global NODE_OPTIONS=--max-old-space-size=4096 umi build",
    "pre": "node initDomain && npm run prefix && cross-env BUILD_ENV=pre ALIAS_ENV=global NODE_OPTIONS=--max-old-space-size=4096 umi build",
    "pro": "node initDomain && npm run prefix && cross-env BUILD_ENV=pro ALIAS_ENV=global NODE_OPTIONS=--max-old-space-size=4096 umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test:coverage": "umi-test --coverage",
    "analyze": "cross-env ANALYZE=1 BUILD_ENV=pro ALIAS_ENV=global umi build",
    "#lint": "eslint src --fix",
    "lint:all": "eslint src/**/*.{js,jsx,tsx,ts} --fix --no-error-on-unmatched-pattern",
    "all": "node initDomain && eslint src --fix && npm run prefix && umi dev"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -e  $GIT_PARAMS"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --parser=typescript --write src/**/*.{js,jsx,tsx,ts}",
      "eslint src --fix",
      "git add ."
    ],
    "*.{less,css}": [
      "prettier --write src/**/*.{less,css}",
      "stylelint --fix src/**/*.{less,css}",
      "git add ."
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@types/html2canvas": "^1.0.0",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-react": "^1.0.6",
    "@wangeditor/plugin-formula": "^1.0.11",
    "@wotu/kp-components": "0.1.21-beta.4",
    "@wotu/pt-components": "1.5.1-beta.0.0.7",
    "@wotu/wotu-components": "1.8.4-beta.0.2.16",
    "@wotu/wotu-pro-components": "0.2.11-beta.0.0.4",
    "ahooks": "^3.7.2",
    "antd": "4.24.13",
    "axios": "0.19.2",
    "classnames": "^2.3.2",
    "clipboard": "^2.0.11",
    "dayjs": "^1.11.2",
    "html2canvas": "^1.4.1",
    "js-cookie": "2.2.0",
    "katex": "^0.16.7",
    "lodash": "^4.17.21",
    "mobx": "6.3.2",
    "mobx-persist": "^0.4.1",
    "mobx-persist-store": "^1.0.6",
    "mobx-react": "7.2.0",
    "mobx-react-lite": "3.2.0",
    "moment": "^2.29.4",
    "nanoid": "^4.0.1",
    "path-to-regexp": "1.8.0",
    "qrcode.react": "^3.1.0",
    "rc-picker": "^3.3.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reflect-metadata": "0.1.13",
    "umi": "3.5.20"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@types/js-cookie": "^3.0.3",
    "@types/lodash": "^4.17.0",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@umijs/fabric": "^2.12.1",
    "@umijs/plugin-esbuild": "1.4.1",
    "@umijs/plugin-qiankun": "2.42.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "3.2.20",
    "amfe-flexible": "^2.2.1",
    "antd-theme-webpack-plugin": "1.3.6",
    "babel-plugin-transform-remove-console": "6.9.4",
    "chalk": "3.0.0",
    "check-prettier": "1.0.3",
    "clean-webpack-plugin": "4.0.0",
    "compression-webpack-plugin": "6.0.2",
    "cross-env": "7.0.3",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "less": "^4.1.3",
    "lint-staged": "^10.0.7",
    "postcss": "^8.4.25",
    "postcss-pxtorem": "5.1.1",
    "stylelint": "^15.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended": "^10.0.1",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "stylelint-less": "^1.0.6",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^3.0.0",
    "typescript": "^4.1.2",
    "webpack": "5.6.0",
    "webpack-cli": "4.2.0",
    "yorkie": "^2.0.0"
  },
  "resolutions": {
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0"
  },
  "#overrides说明#": "淘宝源path-to-regexp1.8.0版本与1.9.0版本错乱处理，勿删",
  "overrides": {
    "path-to-regexp@1.8.0": {
      "resolveFrom": "npm",
      "source": "https://registry.npmjs.org/path-to-regexp/-/path-to-regexp-1.8.0.tgz"
    }
  }
}