{
    "name": "fe-engineer-pc",
    "private": false,
    "description": "工学一体pc",
    "scripts": {
        "prefix": "lessc --js --modify-var=@ant-prefix=fe-engineer-pc --modify-var=@border-radius-base=6px --modify-var=@font-size-base=16px --modify-var=@checkbox-border-radius=2px node_modules/antd/dist/antd.variable.less src/styles/antd.variable.css",
        "start": "node initDomain && npm run prefix && cross-env BUILD_ENV=local ALIAS_ENV=engineer umi dev",
        "dev": "node initDomain && npm run prefix && cross-env NODE_OPTIONS=--max-old-space-size=4096 BUILD_ENV=dev ALIAS_ENV=global umi build",
        "test": "node initDomain && npm run prefix && cross-env NODE_OPTIONS=--max-old-space-size=4096 BUILD_ENV=test ALIAS_ENV=global umi build",
        "pre": "node initDomain && npm run prefix && cross-env NODE_OPTIONS=--max-old-space-size=4096 BUILD_ENV=pre ALIAS_ENV=global umi build",
        "pro": "node initDomain && npm run prefix && cross-env NODE_OPTIONS=--max-old-space-size=4096 BUILD_ENV=pro ALIAS_ENV=global umi build",
        "test:coverage": "umi-test --coverage",
        "analyze": "node initDomain && npm run prefix && cross-env ANALYZE=1 BUILD_ENV=pro ALIAS_ENV=global umi build",
        "lint": "echo \"Please run lint from root directory: pnpm lint\"",
        "lint:all": "echo \"Please run lint:all from root directory: pnpm lint:all\"",
        "prepare": "husky install",
        "turboPipeline": "node initDomain && npm run prefix && umi dev"
    },
    "dependencies": {
        "@ant-design/icons": "5.0.1",
        "@ant-design/pro-layout": "6.5.0",
        "@antv/g2": "^5.4.2",
        "@antv/g6": "5.0.49",
        "@antv/g6-extension-react": "0.1.19",
        "@antv/react-g": "2.0.44",
        "@dnd-kit/core": "^6.0.8",
        "@dnd-kit/sortable": "^7.0.2",
        "@dnd-kit/utilities": "^3.2.1",
        "@microsoft/fetch-event-source": "^2.0.1",
        "@wangeditor/editor": "^5.1.23",
        "@wangeditor/editor-for-react": "^1.0.6",
        "@wotu/pt-components": "1.5.1-beta.0.1.29",
        "@wotu/wotu-components": "1.8.5-beta.0.1.27",
        "add": "^2.0.6",
        "ahooks": "^3.7.0",
        "antd": "^4.24.0",
        "antd-img-crop": "4.13.0",
        "axios": "0.19.2",
        "classnames": "^2.3.2",
        "clipboard": "^2.0.11",
        "dayjs": "^1.11.2",
        "echarts": "5.4.3",
        "echarts-gl": "^2.0.9",
        "html2canvas": "1.4.1",
        "immutability-helper": "^3.1.1",
        "js-cookie": "2.2.0",
        "lodash": "^4.17.21",
        "mobx": "6.3.2",
        "mobx-persist": "^0.4.1",
        "mobx-persist-store": "^1.0.6",
        "mobx-react": "7.2.0",
        "moment": "^2.29.4",
        "qrcode.react": "^3.1.0",
        "qs": "^6.14.0",
        "rc-picker": "^3.5.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-markdown": "^10.1.0",
        "react-slick": "^0.31.0",
        "react-svg": "^16.1.22",
        "reflect-metadata": "0.1.13",
        "rehype-raw": "^7.0.0",
        "remark-gfm": "^4.0.1",
        "simple-mind-map": "0.14.0-fix.1",
        "slick-carousel": "^1.8.1",
        "striptags": "^3.2.0",
        "umi": "3.5.20",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@babel/compat-data": "7.21.4",
        "@commitlint/cli": "^17.8.1",
        "@commitlint/config-conventional": "^17.8.1",
        "@types/react": "^17.0.75",
        "@types/react-dom": "^17.0.25",
        "@types/uuid": "^9.0.1",
        "@types/lodash": "4.14.199",
        "@types/qs": "^6.14.0",
        "@types/react-beautiful-dnd": "^13.1.3",
        "@types/react-slick": "^0.23.13",
        "@umijs/plugin-esbuild": "1.4.1",
        "@umijs/plugin-qiankun": "2.42.0",
        "@umijs/preset-react": "1.x",
        "@umijs/test": "3.2.20",
        "stylelint-config-css-modules": "^4.5.1",
        "stylelint-config-recommended": "^12.0.0",
        "antd-theme-webpack-plugin": "1.3.6",
        "babel-plugin-transform-remove-console": "6.9.4",
        "check-prettier": "1.0.3",
        "clean-webpack-plugin": "4.0.0",
        "compression-webpack-plugin": "6.0.2",
        "cross-env": "7.0.3",
        "eslint": "^8.18.0",
        "husky": "^8.0.3",
        "less": "^4.1.3",
        "lint-staged": "^10.5.4",
        "path-to-regexp": "1.8.0",
        "prettier": "^2.8.8",
        "stylelint": "^15.11.0",
        "typescript": "^4.9.5",
        "webpack": "5.6.0",
        "webpack-cli": "4.2.0"
    },
    "#overrides说明#": "淘宝源path-to-regexp1.8.0版本与1.9.0版本错乱处理，勿删",
    "overrides": {
        "path-to-regexp@1.8.0": {
            "resolveFrom": "npm",
            "source": "https://registry.npmjs.org/path-to-regexp/-/path-to-regexp-1.8.0.tgz"
        }
    },
    "author": "feelingzrs",
    "volta": {
        "node": "16.20.2"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": [
            "prettier --write",
            "eslint --fix"
        ],
        "src/**/*.{less,css}": [
            "prettier --write",
            "stylelint --fix"
        ],
        "src/**/*.{md,json}": [
            "prettier --write"
        ],
        "*.{json,js,ts}": [
            "prettier --write"
        ]
    }
}
